    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Search",
            "skipFiles": ["<node_internals>/**"],
            "program": "${workspaceFolder}/src/examples/search.ts",
            "outFiles": ["${workspaceFolder}/**/*.js"],
            "preLaunchTask": "npm: build",
            "console": "integratedTerminal"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Single stream movie",
            "skipFiles": ["<node_internals>/**"],
            "program": "${workspaceFolder}/src/examples/getSingleStream.ts",
            "outFiles": ["${workspaceFolder}/**/*.js"],
            "preLaunchTask": "npm: build",
            "console": "integratedTerminal"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Series with multiple translators",
            "skipFiles": ["<node_internals>/**"],
            "program": "${workspaceFolder}/src/examples/seriesWithMultipleTranslators.ts",
            "outFiles": ["${workspaceFolder}/**/*.js"],
            "preLaunchTask": "npm: build",
            "console": "integratedTerminal"
        }
    ]
}
